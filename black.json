{
    "C_Cpp.clang_format_style": "Google",
    "editor.renderIndentGuides": false,
    "workbench.colorTheme": "Monokai",
    "editor.fontWeight": "normal",
    "editor.tokenColorCustomizations": {
        "[Monokai]": {
            "comments": "#333333",
            "textMateRules": [
                {
                    "scope": "entity.name.function",
                    "settings": {}
                },
                {
                    "scope": "entity.name.type",
                    "settings": {
                        "fontStyle": "italic bold underline"
                    }
                },
                {
                    "scope": "keyword, variable.language.this",
                    "settings": {
                        "fontStyle": "bold"
                    }
                }
            ]
        }
    },
    "workbench.colorCustomizations": {
        "[Monokai]": {
            "foreground": "#dddddd",
            "menu.background": "#000000",
            "statusBar.background": "#000000",
            "sideBar.background": "#000000",
            "sideBarSectionHeader.background": "#000000",
            "sideBar.foreground": "#555555",
            "list.activeSelectionForeground": "#dddddd",
            "list.inactiveSelectionForeground": "#dddddd",
            "list.warningForeground": "#ffbb00",
            "list.errorForeground": "#ff0000",
            "list.inactiveSelectionBackground": "#111111",
            "list.activeSelectionBackground": "#111111",
            "list.hoverBackground": "#111111",
            "list.focusBackground": "#111111",
            "scrollbarSlider.background": "#50505080",
            "scrollbarSlider.activeBackground": "#50505080",
            "scrollbarSlider.hoverBackground": "#50505080",
            "panel.background": "#000000",
            "editor.background": "#000000",
            "editor.lineHighlightBackground": "#111111",
            "editorGroupHeader.tabsBackground": "#000000",
            "activityBar.background": "#000000",
            "tab.inactiveBackground": "#000000",
            "tab.activeBackground": "#111111",
            "tab.activeBorder": "#a00000",
            "minimap.selectionHighlight": "#a00000",
            "input.background": "#000000",
            "input.border": "#000000",
            "input.placeholderForeground": "#000000",
            "inputOption.activeBorder": "#a00000",
            "inputOption.activeBackground": "#222222",
            "editor.findMatchBorder": "#ffff00",
            "settings.textInputBackground": "#000000",
            "settings.textInputBorder": "#000000",
            "dropdown.background": "#000000",
            "dropdown.border": "#222222",
            "editorWidget.border": "#a00000",
            "editorSuggestWidget.background": "#000000",
            "editorSuggestWidget.border": "#222222",
            "editorSuggestWidget.highlightForeground": "#999999",
            "editorWidget.background": "#222222",
            "editorWidget.foreground": "#dddddd",
            "editorHoverWidget.background": "#222222",
            "editorHoverWidget.border": "#222222",
            "editorLineNumber.foreground": "#555555",
            "editorLineNumber.activeForeground": "#999999",
            "button.background": "#0099ff",
            "button.foreground": "#dddddd",
            "terminal.foreground": "#dddddd",
            "terminal.border": "#dddddd"
        }
    }
}